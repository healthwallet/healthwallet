.sliderViewport {
  border-radius: 5px;
  background-color: #b2dba1;
  width: 60%;
  height: 20px;
  z-index: 9999;
  position: absolute;
  bottom: 0;
  left: 20%;
  right: 20%;
}
.sliderViewport .blockViewport {
  border-radius: 5px;
  background-color: green;
  width: 5%;
  height: 20px;
  z-index: 10000;
  position: absolute;
  bottom: 0;
  left: 50%;
  right: 50%;
}
