#container {
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  position: absolute;
  margin: auto;
  width: 1000px;
  height: 700px;
}
body {
  background-color: black;
}
.dvContainer {
  position: relative;
  width: 100%;
  height: 100%;
  border: 1px solid #424242;
  box-sizing: border-box;
}
.dvContainer .viewer {
  height: 100%;
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
  position: relative;
}
.dvContainer .viewer .viewport {
  width: 100%;
  height: 100%;
  position: relative;
}
.dvContainer .viewer .seriesWindow {
  float: left;
  position: relative;
  box-sizing: border-box;
  overflow: hidden;
}
.dvContainer .viewer .viewportWrapper {
  float: left;
  box-sizing: border-box;
  position: relative;
}
.dvContainer .viewer .viewportWrapper .overlay {
  position: absolute;
  color: #44b9b1;
  font-size: 12px;
}
.dvContainer .viewer .fullScreen {
  position: absolute;
  width: 40px;
  height: 40px;
  bottom: 30px;
  right: 15px;
  z-index: 4000;
}
.dvContainer .viewer .fullScreen span {
  display: block;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  position: absolute;
  margin: auto;
  width: 30px;
  height: 30px;
  color: white;
  line-height: 30px;
  text-align: center;
}
.viewer .imgGalleryBack {
  overflow-x: hidden;
  overflow-y: auto;
  position: absolute;
  left: 0;
  bottom: 60px;
  width: 100%;
}
.viewer .imgGallery {
  width: 100%;
  position: absolute;
  bottom: 0;
  left: 0;
  background-color: black;
}
.viewer .imgGallery .areaBox {
  float: left;
  position: relative;
  width: 33%;
  height: 120px;
  padding: 10px 0;
  text-align: center;
  box-sizing: border-box;
}
.viewer .imgGallery .imgBox {
  margin: 0 auto;
  box-sizing: border-box;
  border: 1px solid #8D9192;
  position: relative;
  width: 100px;
  height: 100px;
}
.viewer .imgGallery .imgBox .img {
  height: 100%;
  width: 100%;
}
.viewer .imgGallery .imgBox .tag {
  position: absolute;
  right: 10px;
  top: 10px;
  width: 50px;
}
.viewer .imgGallery .imgBox .tag > .right {
  display: inline-block;
  float: right;
  font-size: 12px;
  text-align: right;
  color: #3B9DFF;
}
.viewer .imgGallery .imgBox .tagL {
  position: absolute;
  left: 5px;
  top: 10px;
  width: 50px;
}
.viewer .imgGallery .imgBox .tagL > .left {
  display: inline-block;
  float: left;
  font-size: 12px;
  text-align: left;
  color: #E6A300;
}
.viewer .pageBar {
  background-color: #0c0c0c;
  width: 100%;
  height: 50px;
  overflow: hidden;
}
.viewer .bottomPannel {
  width: 100%;
  height: 60px;
  background-color: #323232;
}
