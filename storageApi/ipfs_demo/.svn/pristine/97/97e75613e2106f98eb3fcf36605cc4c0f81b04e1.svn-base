* {
  padding: 0;
  margin: 0;
}
html,
body {
  height: 100%;
}
body {
  font-size: 16px;
  height: 100%;
  font-family: "Microsoft YaHei", "微软雅黑", "Helvetica Neue", "Luxi Sans", "Microsoft JhengHei", "华文细黑", STHeiti, MingLiu;
  color: white;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}
a {
  cursor: pointer;
  text-decoration: none;
}
ul,
ol {
  list-style: none;
}
#unitMask {
  z-index: 9000;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
#unitLayoutSelector,
#unitInnerLayoutSelector {
  position: absolute;
  box-sizing: content-box;
  border: 1px solid #5A5A5A;
  padding: 1px;
  background-color: #5A5A5A;
  z-index: 10000;
}
#unitLayoutSelector .urow,
#unitInnerLayoutSelector .urow {
  margin-left: 0;
  width: 100%;
  margin-right: 0;
}
#unitLayoutSelector .urow:after,
#unitInnerLayoutSelector .urow:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
#unitLayoutSelector .ucol,
#unitInnerLayoutSelector .ucol {
  float: left;
  width: 20px;
  height: 20px;
  margin: 2px;
  background-color: #828282;
  box-sizing: border-box;
  border: 1px solid transparent;
}
#unitLayoutSelector .ucol.lighten,
#unitInnerLayoutSelector .ucol.lighten {
  border: 1px solid white;
  background-color: #B4B4B4;
}
#quickWWWLBox {
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  position: absolute;
  margin: auto;
  z-index: 10000;
  width: 360px;
  height: 400px;
  padding: 0 5px 10px;
  background-color: #E9E7E2;
  font-size: 14px;
}
#quickWWWLBox .topbar {
  height: 30px;
}
#quickWWWLBox .topbar .closeBtn {
  font-size: 14px;
  position: absolute;
  background-color: #FF7D6F;
  color: white;
  top: -2px;
  right: 10px;
  padding: 2px 5px;
}
#quickWWWLBox .calBox {
  background-color: #4D4D4D;
  height: 40px;
  padding: 8px 10px;
}
#quickWWWLBox .calBox .inputArea,
#quickWWWLBox .calBox .yes {
  display: inline-block;
}
#quickWWWLBox .calBox .inputArea {
  width: 120px;
}
#quickWWWLBox .calBox .inputArea input {
  display: inline-block;
  color: black;
  height: 24px;
  padding-left: 8px;
  background-color: #e9e7e2;
  border: 0;
  width: 60px;
}
#quickWWWLBox .calBox .yes {
  cursor: pointer;
  height: 24px;
  line-height: 24px;
  display: inline-block;
  text-align: center;
  background-color: #918b87;
  margin-right: 5px;
  width: 45px;
  color: #e9e7e2;
}
#quickWWWLBox .calBox .yes:hover {
  background-color: #77726e;
}
#quickWWWLBox .ceilBox {
  padding: 5px;
  height: 300px;
  position: relative;
  background-color: #3F3F3F;
  overflow-x: hidden;
  overflow-y: scroll;
  color: #888888;
}
#quickWWWLBox .ceilBox::-webkit-scrollbar {
  /*滚动条，这里不倒腾用户体验了，暂时在满足webkit，这里主要是想节约空间，其实大可不必通过滚动条*/
  width: 8px;
  height: 8px;
}
#quickWWWLBox .ceilBox::-webkit-scrollbar-thumb {
  background: #888888;
  border-radius: 4px;
}
#quickWWWLBox .ceilBox::-webkit-scrollbar-track {
  background: #424242;
  width: 8px;
}
#quickWWWLBox .ceilBox::-webkit-scrollbar-button {
  background-color: #424242;
  height: 4px;
}
#quickWWWLBox .ceilBox .aCeil {
  background-color: #E9E7E2;
  padding: 6px 10px;
  cursor: pointer;
  margin: 8px;
  text-align: center;
  display: block;
  float: left;
}
#quickWWWLBox .ceilBox .aCeil:hover {
  background-color: #bdb7a8;
  color: black;
}
.processingBox {
  box-sizing: border-box;
  width: 100%;
  border-right: 8px solid #888888 ;
  margin: 0 auto;
  position: relative;
  padding-left: 5px;
}
.processingBox .progress {
  margin-bottom: 0;
}
.processingBox .hidebtn {
  font-size: 12px;
  border-radius: 3px;
  line-height: 16px;
  cursor: pointer;
  position: absolute;
  background-color: #FF7D6F;
  color: white;
  top: 2px;
  right: 0px;
  padding: 1px 3px;
}
.processingBox .loadingTxt {
  font-size: 12px;
}
.processingBox .failed {
  font-size: 12px;
  display: inline-block;
}
.processingBox .failed .tryAgain {
  color: #00aaff;
  cursor: pointer;
}
.playSpeedSetBtn {
  position: absolute;
  z-index: 7000;
  width: 40px;
  height: 40px;
  top: 15px;
  right: 15px;
}
.playSpeedSetBtn span {
  display: block;
  width: 35px;
  height: 35px;
  line-height: 35px;
  text-align: center;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  position: absolute;
  margin: auto;
  font-size: 32px;
}
.playSpeedSetBtn:active {
  background-color: #333333;
  opacity: 0.6;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(opacity=(@opacity * 100))";
  filter: "alpha(opacity = (@opacity * 100))";
}
#playSpeedSet {
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  position: absolute;
  margin: auto;
  z-index: 11000;
  width: 300px;
  height: 40px;
  background-color: #0b97c4;
  padding-left: 10px;
  padding-top: 10px;
}
#playSpeedSet .rangeBar,
#playSpeedSet span {
  display: inline-block;
}
#playSpeedSet span {
  text-align: center;
  width: 30px;
  font-size: 14px;
}
#playSpeedSet .rangeBar {
  width: 200px;
}
