.m_btn,.x{cursor:pointer}.overlay_pop{z-index:100004;position:fixed;left:0;top:0;width:100%;height:100%;background-color:rgba(0,0,0,.3);overflow-y:auto}.wmf-confirm{z-index:100005;width:80%;min-height:100px;position:fixed;top:25%;left:50%;margin-top:80px;box-shadow:0 0 10px rgba(0,0,0,.5);background:#fff;border-radius:5px}#load_img,.x{position:absolute}.wmf-confirm .wmf-confirm-title{padding:15px 20px;font-size:14px;font-weight:700;color:#323232;border-bottom:1px solid #dcdcdc}.x{display:block;right:5px;top:1px}.wmf-confirm .wmf-confirm-message{padding:8px 20px;margin-top:15px;font-size:16px;color:#505050}.wmf-confirm .wmf-confirm-btns{padding:15px;text-align:right}.wmf-confirm .wmf-confirm-btns .m_btn{display:inline-block;padding:8px 25px;margin:0 5px}@media (max-width:640px){.wmf-confirm{width:80%;margin-left:-40%}}@media (min-width:641px){.wmf-confirm{width:400px;margin-left:-200px}}.m_btn{display:block;min-width:50px;text-align:center;padding:.7em 1em;line-height:1.3em;text-shadow:none;background:#da251c;border:1px solid #da251c;border-radius:3px;color:#fff}.m_btn.white{background:#f8f8f8;border:1px solid #d7d7d7;color:#505050}.m_btn.black{background:#2b2d32;border:1px solid #2b2d32;color:#fff}#load_overly{display:none;z-index:1001;position:fixed;left:0;top:0;bottom:0;right:0;background-color:rgba(0,0,0,.3);overflow-y:auto}#load_img{z-index:1002;top:50%;margin-top:-32px;left:50%;margin-left:-32px}
