* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
li {
  list-style: none;
}
#container {
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  position: absolute;
  margin: auto;
  height: 100%;
}
.secondMenuActive {
  background: #00bc8c;
  color: #fff;
}
.firstMenuSpanActive {
  color: #00bc8c;
}
.sequenceActive {
  border: 0.1em solid #00bc8c !important;
}
html,
body {
  height: 100%;
}
body {
  overflow: hidden;
}
body ::-webkit-scrollbar {
  width: 0;
  height: 0;
}
.font20 {
  font-size: 20px;
}
.dvContainer {
  position: relative;
  width: 100%;
  height: 100%;
  /*影像窗口*/
}
.dvContainer .viewer {
  background: #000;
  float: left;
  height: 100%;
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
  position: relative;
}
.dvContainer .viewer .rightTool {
  z-index: 2;
  position: absolute;
  right: 0;
  bottom: 0;
  width: 40px;
  margin-right: 10px;
  padding-bottom: 10px;
}
.dvContainer .viewer .rightTool .btn {
  margin-top: 10px;
  margin-bottom: 0 !important;
  float: right;
  opacity: 0.8;
}
.dvContainer .viewer .viewport {
  width: 100%;
  height: 100%;
  position: relative;
}
.dvContainer .viewer .seriesWindow {
  position: relative;
  float: left;
  border: 1px solid #777777;
  box-sizing: border-box;
  overflow: hidden;
}
.dvContainer .viewer .seriesWindow .nextPage,
.dvContainer .viewer .seriesWindow .prevPage {
  padding-top: 9px;
  opacity: 0.5;
  z-index: 2;
  background-color: #fff;
  border-radius: 2px;
  width: 40px;
  height: 50px;
  line-height: 50px;
  text-align: center;
  position: absolute;
}
.dvContainer .viewer .seriesWindow .nextPage img,
.dvContainer .viewer .seriesWindow .prevPage img {
  position: relative;
}
.dvContainer .viewer .seriesWindow .nextPage.checked,
.dvContainer .viewer .seriesWindow .prevPage.checked {
  opacity: 0.8;
}
.dvContainer .viewer .seriesWindow .nextPage {
  left: 10px;
}
.dvContainer .viewer .seriesWindow .prevPage {
  right: 10px;
}
.dvContainer .viewer .seriesWindow.checked {
  border: 2px solid #00bc8c;
}
.dvContainer .viewer .viewportWrapper {
  float: left;
  box-sizing: border-box;
  position: relative;
  border: 2px solid #777777;
}
.dvContainer .viewer .viewportWrapper .overlay {
  position: absolute;
  color: #c8c8c8;
  font-size: 10px;
}
.dvContainer .viewer .fullScreen {
  position: absolute;
  width: 40px;
  height: 40px;
  bottom: 30px;
  right: 15px;
  z-index: 4000;
}
.dvContainer .viewer .fullScreen span {
  display: block;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  position: absolute;
  margin: auto;
  width: 30px;
  height: 30px;
  color: white;
  line-height: 30px;
  text-align: center;
}
.dvContainer .operationBox {
  width: 100%;
  float: left;
  position: absolute;
  bottom: 0;
  /*菜单*/
}
.dvContainer .operationBox .operationBtnGroup {
  background: #f5f5f5;
  width: 100%;
  float: left;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu {
  width: 100%;
  position: relative;
  float: left;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu li {
  width: 20%;
  text-align: center;
  float: left;
  padding: 6px 0;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu li span {
  color: #777;
  display: block;
  margin-top: 6px;
  font-size: 14px;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu li img {
  height: 42px;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .secondMenu {
  white-space: nowrap;
  overflow: hidden;
  overflow-x: auto;
  z-index: 3;
  width: 100%;
  position: absolute;
  top: -80px;
  height: 80px;
  display: none;
  background: #ecfffb;
  padding: 20px 10px;
  text-align: center;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .secondMenu .secondMenuSpan {
  padding-top: 0 !important;
  margin-top: 0 !important;
  display: inline-block;
  height: 40px;
  line-height: 40px;
  padding: 0 20px;
  border: 0.1em solid #00bc8c;
  color: #00bc8c;
  font-size: 14px;
  border-radius: 2px;
  margin-left: 10px;
  margin-right: 10px;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .secondMenu span.checked {
  border: 0.1em solid #ecfffb !important;
  background: -webkit-gradient(linear, 0% 0%, 100% 0%, from(#00bc8c), to(#11f0c3));
  color: #ecfffb;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .sequenceMenu {
  z-index: 2;
  position: absolute;
  top: -80px;
  background: #000;
  width: 100%;
  height: 80px;
  padding: 10px 5px;
  border-top: 1px solid #00bc8c;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .sequenceMenu .imgBox {
  position: relative;
  display: block;
  width: 60px;
  height: 60px;
  margin: 0 5px;
  float: left;
  border: 0.1em solid #ecfffb;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .sequenceMenu .imgBox .img {
  height: 100%;
  width: 100%;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .sequenceMenu .imgBox .tagBottom {
  position: absolute;
  bottom: 0;
  left: 2px;
  color: #c8c8c8;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .sequenceMenu .imgBox .tagBottom span {
  float: left;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .sequenceMenu .imgBox.checked,
.dvContainer .operationBox .operationBtnGroup .firstMenu .sequenceMenu .imgBox:hover {
  border: 0.1em solid #00bc8c !important;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .scrollBox {
  display: none;
  position: absolute;
  top: -80px;
  z-index: 4;
  width: 100%;
  height: 80px;
  background: #000;
  float: left;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .scrollBox img {
  margin-top: 0px !important;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .scrollBox .speed {
  position: absolute;
  font-weight: bold;
  font-size: 20px;
  margin: auto;
  height: 80px;
  line-height: 76px;
  text-align: center;
  color: #ecfffb;
  top: 0;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .scrollBox .speed.checked {
  color: #00bc8c;
  background-color: #ecfffb;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .scrollBox .scrollBarBox {
  height: 80px;
  width: 100%;
  line-height: 80px;
  text-align: center;
  float: left;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .scrollBox .scrollBarBox .scrollBar {
  width: 100%;
  margin-top: 38px;
  position: relative;
  height: 4px;
  background: #ecfffb;
  border-radius: 2px;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .scrollBox .scrollBarBox .scrollBar .rectBox {
  top: -38px;
  position: absolute;
  left: -1px;
  width: 40px;
  height: 80px;
  line-height: 0 !important;
  text-align: center;
}
.dvContainer .operationBox .operationBtnGroup .firstMenu .scrollBox .scrollBarBox .scrollBar .rectBox .rect {
  width: 40px;
  height: 20px;
  margin-top: 30px !important;
}
@media screen and (max-height: 736px) {
  .firstMenu img {
    margin-top: 2px;
    height: 20px !important;
  }
  .firstMenu span {
    margin-top: 5px !important;
    font-size: 14px !important;
  }
}
#papaFrame {
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 109000;
}
#papaFrame .bg {
  z-index: 108999;
  background-color: white;
  height: 100%;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0.5;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(opacity=(@opacity * 100))";
  filter: "alpha(opacity = (@opacity * 100))";
}
#papaFrame .mprClose {
  position: absolute;
  top: 10px;
  right: 20px;
  cursor: pointer;
  color: red;
  background-color: #CCCCCC;
  width: 30px;
  height: 30px;
  z-index: 109002;
  font-size: 30px;
  line-height: 30px;
  text-align: center;
}
#papaFrame .mprSmall {
  position: absolute;
  top: 10px;
  right: 60px;
  cursor: pointer;
  color: black;
  font-weight: 700;
  background-color: #CCCCCC;
  width: 30px;
  height: 30px;
  z-index: 109002;
  font-size: 30px;
  line-height: 30px;
  text-align: center;
}
#papaFrame #mprFrame {
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  position: absolute;
  margin: auto;
  background-color: white;
  z-index: 109001;
}
